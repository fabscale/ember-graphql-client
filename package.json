{
  "version": "0.1.3",
  "license": "MIT",
  "private": true,
  "repository": "https://github.com/fabscale/ember-graphql-client",
  "workspaces": [
    "packages/client",
    "packages/mock",
    "packages/release-it-generate-types"
  ],
  "scripts": {
    "lint:js": "eslint . --cache"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "~4.28.0",
    "@typescript-eslint/parser": "~4.28.0",
    "@ember-graphql-client/release-it-generate-types": "*",
    "babel-eslint": "~10.1.0",
    "eslint": "~7.31.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-ember": "~10.5.1",
    "eslint-plugin-ember-es6-class": "~0.0.1",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-prettier": "~3.4.0",
    "eslint-plugin-qunit": "~6.2.0",
    "prettier": "~2.3.2",
    "release-it": "~14.10.0",
    "release-it-lerna-changelog": "~3.1.0",
    "release-it-yarn-workspaces": "~2.0.1"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "release-it": {
    "plugins": {
      "@ember-graphql-client/release-it-generate-types": {},
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md"
      },
      "release-it-yarn-workspaces": {}
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true
    },
    "npm": false
  },
  "volta": {
    "node": "14.16.1",
    "yarn": "1.22.10"
  }
}
